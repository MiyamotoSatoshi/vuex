@import '../bootstrap'
@import '../theme'

/* Themes */
v-paper($material)
  background-color: $material.cards
  border-color: $material.cards
  color: $material.text.primary

theme(v-paper, "v-paper")

.v-paper
  display: block
  border-radius: $paper-border-radius
  position: relative

  &--tile
    border-radius: 0
