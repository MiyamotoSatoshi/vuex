{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": true,
  "build": {
    "env": {
      "VUE_APP_COSMIC_BUCKET_SLUG": "@cosmic-bucket-slug",
      "VUE_APP_COSMIC_BUCKET_READ_KEY": "@cosmic-bucket-read-key"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index"
    }
  ],
  "github": {
    "autoAlias": false,
    "silent": true
  }
}
