    "vue-router": "^3.1.6",
    "vuetify": "^2.2.11",
    "vuex": "^3.4.0",
    "webpack-node-externals": "^2.5.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.3",
    "@vue/cli-plugin-eslint": "~4.5.4",
    "@vue/cli-plugin-pwa": "^4.5.4",
